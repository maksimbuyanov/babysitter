{
  "name": "home-babysitter",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev:server": "nodemon --config ./serverNodemon.json",
    "dev:bot": "nodemon --config ./botNodemon.json",
    "seq:migrate:up": "npx sequelize-cli db:migrate",
    "seq:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seq:migrate:generate": "npx sequelize-cli migration:generate --name",
    "copyStatic" : "copyfiles -u 1 src/**/*.{html,css} dist"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.6.3",
    "node": "^21.1.0",
    "sequelize": "^6.34.0",
    "telegraf": "^4.15.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.3.2",
    "prettier": "^3.0.3"
  }
}
